{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
        "no-console": [false],
        "no-string-throw": true,
        "no-unused-expression": true,
        "no-unused-variable": true,
        "no-duplicate-variable": true,
        "curly": true,
        "class-name": true,
        "triple-equals": true,
        "trailing-comma": false,
        "no-empty": false
    },
    "rulesDirectory": []
}
